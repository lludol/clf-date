{
  "name": "clf-date",
  "version": "0.0.4",
  "description": "Return the current date in CLF format",
  "main": "./out/main.js",
  "bin": {
    "clf-date": "./bin/clf-date.js"
  },
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir out",
    "cover": "istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build",
    "test": "mocha test/index.js --compilers js:babel-core/register"
  },
  "keywords": [
    "clf-date",
    "clf",
    "common-log-format",
    "common",
    "log",
    "format"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lludol/clf-date.git"
  },
  "author": "lludol",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "istanbul": "^0.4.4",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
